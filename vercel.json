{
  "buildCommand": "pnpm vercel-build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
  },
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  }
}